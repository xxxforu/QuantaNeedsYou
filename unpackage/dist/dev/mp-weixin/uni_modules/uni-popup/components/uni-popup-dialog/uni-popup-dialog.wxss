.uni-popup-dialog{width:300px;background-color:#fff;border-radius:10px;border:1px #ffa265 solid;box-shadow:0 3px 8px rgba(50,49,49,.06)}.uni-dialog-title{display:flex;color:#707070;font-weight:700;flex-direction:row;justify-content:center;padding-top:25px}.uni-dialog-title-text{font-size:16px;font-weight:700;color:#707070}.uni-dialog-content{color:#707070;display:flex;flex-direction:row;justify-content:center;align-items:center;padding:20px}.uni-dialog-content-text{font-size:14px;color:#6c6c6c}.uni-dialog-button-group{display:flex;flex-direction:row;border-top-color:#f5f5f5;border-top-style:solid;border-top-width:1px}.uni-dialog-button{display:flex;flex:1;flex-direction:row;justify-content:center;align-items:center;height:45px}.uni-border-left{border-left-color:#f0f0f0;border-left-style:solid;border-left-width:1px}.uni-dialog-button-text{font-size:16px;color:#333}.uni-button-color{color:#ffa265}.uni-dialog-input{flex:1;font-size:14px;border:1px #eee solid;height:40px;padding:0 10px;border-radius:5px;color:#555}.uni-popup__success{color:#4cd964}.uni-popup__warn{color:#f0ad4e}.uni-popup__error{color:#dd524d}.uni-popup__info{color:#707070}
